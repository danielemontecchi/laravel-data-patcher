{
    "name": "danielemontecchi/laravel-data-patcher",
    "description": "Execute data patches after migrations in Laravel.",
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "DanieleMontecchi\\LaravelDataPatcher\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanieleMontecchi\\LaravelDataPatcher\\DataPatcherServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^8.0"
    },
    "minimum-stability": "stable",
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
